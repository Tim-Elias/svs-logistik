<div class="container">
    <div class="col-md-5 pull-center">
        <form class="form-box">
            <h3 class="text-center">Задайте Ваш вопрос</h3>
            <div class="form-group">
                <div class="col-md-12">
                    <input type="text" class="form-control" id="reName" placeholder="Имя">

                </div>
            </div>
            <div class="form-group">
                <div class="col-md-12">
                    <input type="text" class="form-control" id="reEmail" placeholder="Email">
